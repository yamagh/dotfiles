[user]
	name = yamagh
	email = hiroyuki.yamagami@gmail.com
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[alias]
	ch = checkout
	st = status
	br = branch
	co = commit
[push]
	default = matching
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
